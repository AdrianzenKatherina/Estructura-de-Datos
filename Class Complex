class Complex:  #Tiene una parte imaginaria y otra real
    def __init__(self, realpart, imagpart):
        self.r = realpart
        self.i = imagpart
    
re = float(input("Ingrese la parte real: "))
im= float(input("Ingrese la parte imaginaria: "))
#Crea en el objeto x de la clase Complex -  instancia en python
x = Complex(re, im)       
print("Real: ", x.r, "Imaginario: ", x.i)
